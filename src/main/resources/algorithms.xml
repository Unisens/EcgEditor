<?xml version="1.0" encoding="UTF-8"?>
<ecgeditor>
	<algorithm name="MuGuard" class="de.fzi.ekgdemonstrator.algorithms.AlgorithmJNI">
		<input>
			<signal name="EKG Daten" contentClass="ECG">
				<configset samplerate="250" lsb="1" unit="uV" delay="339" />
				<channel name="CH1" />
				<channel name="CH2" />
			</signal>
			<signal name="ACC Daten" contentClass="ACC">
				<configset samplerate="50" lsb="1" unit="uV" delay="339" />
				<channel name="X" />
				<channel name="Y" />
				<channel name="Z" />
			</signal>
		</input>
		<output>
			<timerange name="Artifact"  /> 
			<signal name="filtered ECG" contentClass="ECG" />
			<event name="QRS" />
			<heartrate name="Heartrate" />
		</output>
	</algorithm>
	<algorithm name="OseaQrsDet" class="de.fzi.ekgeditor.algorithm.jni.OseaQrsDetAlgorithmJNI">
		<input>
			<signal name="EKG Daten" contentClass="ECG">
				<configset samplerate="200" lsb="1" unit="uV" delay="200" />
				<configset samplerate="250" lsb="1" unit="uV" delay="200" />
				<configset samplerate="256" lsb="1" unit="uV" delay="200" />
				<configset samplerate="400" lsb="1" unit="uV" delay="200" />
				<configset samplerate="500" lsb="1" unit="uV" delay="200" />
				<configset samplerate="512" lsb="1" unit="uV" delay="200" />
				<channel name="CH1" />
			</signal>
		</input>
		<output>
			<signal name="EKG Daten" contentClass="ECG" />
			<event name="QRS-Trigger" />
		</output>
	</algorithm>
	<algorithm name="OseaQrsDet2" class="de.fzi.ekgeditor.algorithm.jni.OseaQrsDet2AlgorithmJNI">
		<input>
			<signal name="EKG Daten" contentClass="ECG">
				<configset samplerate="200" lsb="1" unit="uV" delay="200" />
				<configset samplerate="250" lsb="1" unit="uV" delay="200" />
				<configset samplerate="256" lsb="1" unit="uV" delay="200" />
				<configset samplerate="400" lsb="1" unit="uV" delay="200" />
				<configset samplerate="500" lsb="1" unit="uV" delay="200" />
				<configset samplerate="512" lsb="1" unit="uV" delay="200" />
				<channel name="CH1" />
			</signal>
		</input>
		<output>
			<signal name="EKG Daten" contentClass="ECG" />
			<event name="QRS-Trigger" />
		</output>
	</algorithm>
	<algorithm name="OSEA-Java" class="de.fzi.ekgdemonstrator.algorithms.AlgorithmJNI">
		<input>
			<signal name="EKG Daten" contentClass="ECG">
				<configset samplerate="250" lsb="1" unit="uV" delay="250" />
				<channel name="Einthoven I" />
				<channel name="Einthoven II" />
			</signal>
		</input>
		<output>
			<signal name="ECG" />
			<event name="QRS" />
			<event name="PVC" />
			<event name="UNKNOWN" />
		</output>
	</algorithm>
