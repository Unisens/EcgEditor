<?xml version="1.0" encoding="UTF-8"?>
<ecgeditor>
	<algorithm name="OseaQrsDet" class="de.fzi.ekgeditor.algorithm.jni.OseaQrsDetAlgorithmJNI">
		<input>
			<signal name="EKG Daten" contentClass="ECG">
				<configset samplerate="200" lsb="1" unit="uV" delay="200" />
				<configset samplerate="250" lsb="1" unit="uV" delay="200" />
				<configset samplerate="256" lsb="1" unit="uV" delay="200" />
				<configset samplerate="400" lsb="1" unit="uV" delay="200" />
				<configset samplerate="500" lsb="1" unit="uV" delay="200" />
				<configset samplerate="512" lsb="1" unit="uV" delay="200" />
				<configset samplerate="1024" lsb="1" unit="uV" delay="200" />
				<channel name="CH1" />
			</signal>
		</input>
		<output>
			<signal name="EKG Daten" contentClass="ECG" />
			<event name="QRS-Trigger" />
		</output>
	</algorithm>

	<algorithm name="OseaQrsDet2" class="de.fzi.ekgeditor.algorithm.jni.OseaQrsDet2AlgorithmJNI">
